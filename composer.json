{
    "name": "proghasan/blade-form-validator",
    "description": "Validate Laravel forms, from Blade files",
    "keywords": [
        "laravel",
        "blade-form-validator",
        "validation",
        "larave-form-validation",
        "larave-form-validator",
        "form-validation"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "illuminate/support": "^12.0",
        "orchestra/testbench": "^10.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Proghasan\\BladeFormValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proghasan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Proghasan\\BladeFormValidator\\BladeFormValidateServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "proghasan",
            "email": "mohammaddothasan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
