{
    "name": "proghasan/blade-form-validator",
    "description": "Laravel form validation",
    "version": "v1.0.0",
    "type": "library",
    "require": {
        "php": "^8.3.0",
        "orchestra/testbench": "^10.3",
        "illuminate/support": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Proghasan\\BladeFormValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proghasan\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "proghasan",
            "email": "mohammaddothasan@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Proghasan\\BladeFormValidator\\BladeFormValidateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
