{
    "name": "proghasan/blade-form-validator",
    "description": "Laravel form validation",
    "type": "composer-plugin",
    "require": {
        "php": "^8.3.0",
        "orchestra/testbench": "^10.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Proghasan\\BladeFormValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "proghasan",
            "email": "mohammaddothasan@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
